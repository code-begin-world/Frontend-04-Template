    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Program moc",
            "program": "${workspaceFolder}/app.js",
            "runtimeArgs": [
                "--require", "@babel/register"
            ],
            "sourceMaps": true,
            "args": [],
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "pwa-node"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch File",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${file}"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\index.js"
        }
    ]
}